# 🐺 Projekt Cerberus Phoenix v2.0 - Environment Variables
# Skopiuj ten plik jako .env i dostosuj wartości

# 🌐 Solana Configuration
SOLANA_RPC_URL=https://api.devnet.solana.com
SOLANA_COMMITMENT=confirmed

# 🚀 Jito Configuration
JITO_BLOCK_ENGINE_URL=https://mainnet.block-engine.jito.wtf
JITO_TIP_AMOUNT=10000

# 🧠 AI Configuration
FINLLAMA_API_URL=http://localhost:11434
DEEPSEEK_API_URL=http://localhost:11435

# 🗄️ Qdrant Configuration
QDRANT_URL=http://qdrant:6333
QDRANT_COLLECTION=cerberus_context

# 🔐 API Keys (WYMAGANE - ustaw własne klucze)
BIRDEYE_API_KEY=your_birdeye_api_key_here
OUMI_API_KEY=your_oumi_api_key_here
HELIUS_API_KEY=your_helius_api_key_here
HELIUS_AUTH_TOKEN=your_helius_webhook_auth_token_here

# 🔄 Multi-RPC Configuration (FREE Providers Only)
# Real production configuration for DEVNET and MAINNET

# 🌐 Network Configuration
SOLANA_NETWORK=mainnet-beta
# Options: mainnet-beta, devnet, testnet

# 🌟 Helius API Pro (Enhanced Data + Webhooks) - FREE TIER
HELIUS_API_KEY=your_helius_api_key_here
HELIUS_BASE_URL=https://api.helius.xyz
HELIUS_MONTHLY_LIMIT=100000
# FREE: 100k requests/month (NOT 1M - that's paid)

# 🔮 Alchemy (Stable Performance) - FREE TIER
ALCHEMY_API_KEY=Wu2Kqfk_50kW_Zs4ifjuf3c7afxLOs7R
ALCHEMY_MAINNET_URL=https://solana-mainnet.g.alchemy.com/v2/Wu2Kqfk_50kW_Zs4ifjuf3c7afxLOs7R
ALCHEMY_DEVNET_URL=https://solana-devnet.g.alchemy.com/v2/Wu2Kqfk_50kW_Zs4ifjuf3c7afxLOs7R
ALCHEMY_MONTHLY_LIMIT=100000

# 🌐 Public Solana RPC (FREE, Unlimited)
PUBLIC_MAINNET_RPC=https://api.mainnet-beta.solana.com
PUBLIC_DEVNET_RPC=https://api.devnet.solana.com

# 🔄 Multi-RPC Routing Strategy (3 FREE providers)
RPC_ROUTING_STRATEGY=cost_optimized
ENABLE_RPC_FAILOVER=true
API_USAGE_ALERT_THRESHOLD=0.8
TOTAL_FREE_REQUESTS_MONTHLY=200000

# 🦈 Piranha Strategy Configuration (Small Portfolio)
MAX_POSITION_SIZE_SOL=0.1
ENABLE_JITO=true

# 🌐 Frontend Configuration
NEXT_PUBLIC_API_URL=http://localhost:8080
NEXT_PUBLIC_NINJA_URL=http://localhost:8081

# 📊 Monitoring
PROMETHEUS_RETENTION_TIME=30d
GRAFANA_ADMIN_PASSWORD=admin

# 🔧 Development Settings
DEV_MODE=true
RUST_LOG=debug
NODE_ENV=development

# 🎣 Webhook Configuration
KESTRA_TRIGGER_URL=http://kestra:8080/api/v1/executions/trigger/helius-webhook
WEBHOOK_RATE_LIMIT=100

